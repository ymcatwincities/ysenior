{
  "name": "drupal/tqextension",
  "type": "behat-extension",
  "description": "Behat extension for testing Drupal sites",
  "keywords": ["drupal", "web", "test", "behat", "testing", "extension"],
  "license": "MIT",
  "authors": [{
    "name": "Sergii Bondarenko",
    "email": "sb@firstvector.org"
  }],
  "require": {
    "php": ">=5.5",
    "behat/environment-loader": "1.*",
    "behat/debug-extension": "1.*",
    "drupal/drupal-extension": "3.*"
  },
  "require-dev": {
    "drush/drush": "8.*",
    "phpunit/phpunit": "4 - 5"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\TqExtension\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\TqExtension\\": "tests/phpunit"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
